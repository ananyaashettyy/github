git clone
cd
git add .
git commit -m ""
git push

git branch feature-branch
git checkout feature-branch
git add .
git commit -m ""
git push
git push (upstream)
git checkout main
git merge feature-branch
git add .
git commit -m ""
git push

git stash
git checkout feature-branch
git stash apply
git branch -v
git fetch origin
git branch -v
git checkout feature-branch
git rebase main

git log
